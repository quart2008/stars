SetPointer (HourGlass!)

MDI_Main_Frame.SetMicroHelp('Creating PIMR File.  Please wait')

// SAH 05/15/02 Track 3064 -Begin
IF fx_create_pimr_file() = 0 THEN 
	MessageBox ('Error', 'PIMR file was not created')
