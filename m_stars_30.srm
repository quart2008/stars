event clicked;// 04/11/96 FDG 	Prob 42 (Stars31) - Added
//	06/03/98	FDG	Track 1295.  Get the specified print d/w in case there
//						is a tab folder or user object
//	05/01/01	GaryR	Stars 4.7 DataBase Port - Eliminate view CODE_DESC

GraphicObject	ldo_HasFocus
Integer			li_dws = 0
Integer			li_max,	li_i
w_master			lw_sheet
DataWindow		ldw,	ldw_temp

SetPointer (HourGlass!)

	// Get the active sheet
lw_sheet		=	ParentWindow.GetActiveSheet()

IF	NOT	IsValid(lw_sheet)		Then
	SetPointer (Arrow!)
	Return
END IF

ldw_temp	=	lw_sheet.of_GetPrintDW()

IF	IsValid (ldw_temp)		THEN
	ldw			=	ldw_temp
	li_dws ++
ELSE
	li_max		=	UpperBound (lw_sheet.Control[])

	// Determine how many visible datawindows exist on this window
	//	(excluding the print criteria datawindow).

	FOR	li_i	=	1	TO	li_max
		IF	TypeOf (lw_sheet.Control[li_i])	=	DataWindow!		&
		AND lw_sheet.Control[li_i].visible	=	TRUE				THEN
				// Ignore the print criteria datawindow
			ldw_temp	=	lw_sheet.Control[li_i]
			//	05/01/01	GaryR	Stars 4.7 DataBase Port - Begin
			//IF	ldw_temp.DataObject	<>	'd_criteria_print_text_dw'	THEN
				ldw	=	ldw_temp
				li_dws++
			//END IF
			//	05/01/01	GaryR	Stars 4.7 DataBase Port - End
		END IF
	NEXT
END IF

	// If there are no visible datawindows, then don't do anything
IF	li_dws	=	0		THEN
	SetPointer (Arrow!)
	Return
END IF

	// If 1 datawindow was found, then open w_printzoom.  Otherwise,
	// determine which datawindow has focus.

IF	li_dws	=	1		THEN
	SetPointer (Arrow!)
	OpenWithParm (w_printzoom, ldw)
	Return
ELSE		//	There is more than 1 datawindow.  See which one has focus
	ldo_HasFocus	=	GetFocus()
	IF	ldo_HasFocus.TypeOf()	=	DataWindow!				THEN
		ldw			=	ldo_HasFocus
		//	05/01/01	GaryR	Stars 4.7 DataBase Port - Begin
		//IF	ldw.dataobject	<>	'd_criteria_print_text_dw'	THEN
			SetPointer (Arrow!)
			OpenWithParm (w_printzoom, ldw)
			Return
		//END IF
		//	05/01/01	GaryR	Stars 4.7 DataBase Port - End
	END IF
END IF
		
SetPointer (Arrow!)

end event

