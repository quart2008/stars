	ls_classname	=	lw_parent.ClassName()

	IF	Upper ( ls_classname )	=	'W_QUERY_ENGINE'		THEN
		luo_query	=	lw_parent.dynamic	wf_GetActiveQuery()
	END IF
End If
//AJS 07-13-98 4.0 end

fx_crystal_reports(luo_Query,'VIEW')
