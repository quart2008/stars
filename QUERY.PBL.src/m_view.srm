public subroutine of_stats ();//*********************************************************************************
// Script Name:	m_menu.m_statistics.m_sum.clicked
//
// Arguments:	None
//
// Returns:		None
//
// Description:	This script calls the script that executes the statistics logic
//
//*********************************************************************************
//
//	09/10/09	GaryR	QEN.650.5229.005	Add statisitics to QE report
//
//*********************************************************************************

iw_query_engine	=	This.ParentWindow

If IsValid(iw_query_engine) Then	iu_query	=	iw_query_engine.wf_GetActiveQuery()
If IsValid(iu_Query) Then iu_query.Event	ue_tabpage_view_stats()
end subroutine

public subroutine of_reset_stats ();//*********************************************************************************
// Script Name:	m_menu.m_statistics.m_sum.clicked
//
// Arguments:	None
//
// Returns:		None
//
// Description:	This script unchecks all statistics menu items
//
//*********************************************************************************
//
//	09/10/09	GaryR	QEN.650.5229.005	Add statisitics to QE report
//
//*********************************************************************************

m_menu.m_statistics.m_sum.checked = FALSE
m_menu.m_statistics.m_avg.checked = FALSE
m_menu.m_statistics.m_med.checked = FALSE
m_menu.m_statistics.m_mode.checked = FALSE
m_menu.m_statistics.m_min.checked = FALSE
m_menu.m_statistics.m_max.checked = FALSE
m_menu.m_statistics.m_std.checked = FALSE
m_menu.m_statistics.m_var.checked = FALSE
m_menu.m_statistics.m_all.checked = FALSE
end subroutine

